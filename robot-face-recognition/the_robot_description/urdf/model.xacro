<?xml version="1.0" ?>
<robot name="the_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- <xacro:include filename="$(find robot-face-recognition-project)/the_robot_description/urdf/model_mobile_base.xacro"/> -->

  <xacro:include filename="$(find robot-face-recognition-project)/the_robot_description/urdf/model.gazebo"/>

 <xacro:property name="the_robot__camera_link" value="0.05" /> <!-- Size of square 'camera' box -->

  <joint name="the_robot__left_wheel_hinge" type="continuous"><!--changement, avant c'etait "fixed" au lieu de "revolute"-->
    <parent link="the_robot__chassis"/>
    <child link="the_robot__left_wheel"/>
    <origin rpy="0     1.5707 1.5707" xyz="0.79999 0.56999 0.12499"/>
    <axis xyz="9.e-05 9.e-05 1.e+00"/>
    <limit effort="0.0" lower="0.0" upper="0.0" velocity="0.0"/>
    <!--<limit effort="300" lower="-0.5" upper="0.5" velocity="12"/>--> 
  </joint>
  <joint name="the_robot__right_wheel_hinge" type="continuous"><!--changement, avant c'etait "fixed au lieu de "revolute"-->
    <parent link="the_robot__chassis"/>
    <child link="the_robot__right_wheel"/>
    <origin rpy="0     1.5707 1.5707" xyz="0.8     -0.56999  0.125"/>
    <axis xyz="9.e-05 9.e-05 1.e+00"/>
    <limit effort="0.0" lower="0.0" upper="0.0" velocity="0.0"/>
  </joint>

<transmission name="left_wheel_transmission">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="the_robot__left_wheel_hinge">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>  
  </joint>
  <actuator name="left_wheel_actuator">
    <mechanicalReduction>7</mechanicalReduction>
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </actuator>
</transmission>

<transmission name="right_wheel_transmission">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="the_robot__right_wheel_hinge">
    <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>  
  </joint>
  <actuator name="right_wheel_actuator">
    <mechanicalReduction>7</mechanicalReduction>
    <hardwareInterface>VelocityJointInterface</hardwareInterface>
  </actuator>
</transmission>

