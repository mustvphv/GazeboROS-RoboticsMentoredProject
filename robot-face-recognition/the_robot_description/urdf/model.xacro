  <joint name="the_robot__palm_left_finger" type="revolute">
    <parent link="the_robot__palm"/>
    <child link="the_robot__left_finger"/>
    <origin rpy="0       0      -0.78539" xyz="0.1 0.2 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-0.8" upper="0.8" velocity="1000.0"/>
  </joint>
  <joint name="the_robot__left_finger_tip" type="revolute">
    <parent link="the_robot__left_finger"/>
    <child link="the_robot__left_finger_tip"/>
    <origin rpy="0      0      2.35609" xyz="0.09616 0.23758 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-0.8" upper="0.8" velocity="1000.0"/>
  </joint>
  <joint name="the_robot__palm_right_finger" type="revolute">
    <parent link="the_robot__palm"/>
    <child link="the_robot__right_finger"/>
    <origin rpy="0      0      0.78539" xyz="0.1 -0.2  0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-0.8" upper="0.8" velocity="1000.0"/>
  </joint>
  <joint name="the_robot__right_finger_tip" type="revolute">
    <parent link="the_robot__right_finger"/>
    <child link="the_robot__right_finger_tip"/>
    <origin rpy="0      0      0.78531" xyz="0.09616 -0.23758  0"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-0.8" upper="0.8" velocity="1000.0"/>
  </joint>
  <joint name="the_robot__palm_riser" type="prismatic">
    <parent link="the_robot__riser"/>
    <child link="the_robot__palm"/>
    <origin rpy="0 0 0" xyz="0  0   -0.4"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="0.0" upper="0.9" velocity="1000.0"/>
  </joint>
    <gripper name="simple_gripper">
      <grasp_check>
        <detach_steps>40</detach_steps>
        <attach_steps>20</attach_steps>
        <min_contact_count>2</min_contact_count>
      </grasp_check>
      <gripper_link>the_robot__right_finger</gripper_link>
      <gripper_link>the_robot__right_finger_tip</gripper_link>
      <gripper_link>the_robot__left_finger</gripper_link>
      <gripper_link>the_robot__left_finger_tip</gripper_link>
      <palm_link>the_robot__palm</palm_link>
    </gripper>
  <joint name="the_robot__camera_joint" type="fixed">
    <parent link="the_robot__riser"/>
    <child link="the_robot__camera_link"/>
    <origin rpy="0 0 0" xyz="-0.08  0     0.60"/>
    <axis xyz="0 0 0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
  <joint name="the_robot__arm_gripper_joint" type="fixed">
    <parent link="the_robot__chassis"/>
    <child link="the_robot__riser"/>
    <origin rpy="0 0 0" xyz="1.02499 0      0.5"/>
    <axis xyz="0 0 0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
  <joint name="the_robot__hokuyo_joint" type="fixed">
    <parent link="the_robot__palm"/>
    <child link="the_robot__hokuyo_link"/>
    <origin rpy="0 0 0" xyz="0  0    0.2"/>
    <axis xyz="0 0 0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
  <link name="the_robot__chassis">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="-0.1  0  -0.1"/>
      <inertia ixx="0.5" ixy="0" ixz="0" iyy="1.0" iyz="0" izz="0.1"/>
    </inertial>
    <collision name="the_robot__collision">
      <!--<origin rpy="0 0 0" xyz="0   0   0.25"/>-->
      <origin rpy="0 0 0" xyz="-0.2   0   0.25"/>
      <geometry>
        <box size="2 1 0.3"/>
      </geometry>
    </collision>
    <collision name="the_robot__caster_collision">
      <!--<origin rpy="0 0 0" xyz="-0.8    0     0.125"/>-->
      <origin rpy="0 0 0" xyz="-1    0     0.125"/>
      <geometry>
        <sphere radius=".125"/>
      </geometry>
    </collision>
    <visual name="the_robot__visual1">
      <origin rpy="0 0 0" xyz="-0.2   0   0.25"/>
      <geometry>
        <box size="2 1 0.3"/>
      </geometry>
    </visual>
    <visual name="the_robot__caster_visual">
      <origin rpy="0 0 0" xyz="-1    0     0.125"/>
      <geometry>
        <sphere radius=".125"/>
      </geometry>
    </visual>
  </link>
  <link name="the_robot__left_wheel">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <collision name="the_robot__collision">
      <origin rpy="0 0 0" xyz="0   0   0.03"/>
      <geometry>
        <cylinder length=".05" radius=".125"/>
      </geometry>
    </collision>
    <visual name="the_robot__visual2">
